def count_vowels(string):
    vowels = {'a', 'e', 'i', 'o', 'u'}
    counts = {vowel: 0 for vowel in vowels}

    for char in string.lower():
        if char in vowels:
            counts[char] += 1

    return counts

string = ("egor and Stefan are practicing programming")
print("hi")
print(count_vowels(string))
